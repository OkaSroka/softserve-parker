{
  "name": "parker-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node-dev --no-notify app/main.ts",
    "build": "tsc",
    "db:migrate": "db-migrate up",
    "db:migrate-down": "db-migrate down",
    "db:create-migration": "db-migrate create",
    "ci:lint": "eslint \"app/**/*.ts\" --max-warnings 1",
    "ci:prettier": "prettier \"app/**/*.ts\" --check",
    "test": "NODE_ENV=development jest"
  },
  "//": "DO NOT REMOVE the graphql package - it's not used in code but it's a peer dependency",
  "dependencies": {
    "apollo-server-express": "2.9.15",
    "bcrypt": "3.0.7",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "date-fns": "2.8.1",
    "db-migrate": "0.11.6",
    "db-migrate-mysql": "2.1.0",
    "dotenv": "8.2.0",
    "envalid": "6.0.0",
    "express": "4.17.1",
    "graphql": "14.5.8",
    "helmet": "3.21.2",
    "jsonwebtoken": "8.5.1",
    "mysql2": "2.1.0",
    "nodemailer": "6.4.2",
    "reflect-metadata": "0.1.13",
    "type-graphql": "0.17.6",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/express": "4.17.1",
    "@types/graphql": "^14.0.7",
    "@types/jest": "24.0.25",
    "@types/jsonwebtoken": "8.3.5",
    "@types/mysql2": "github:types/mysql2",
    "@typescript-eslint/eslint-plugin": "2.15.0",
    "@typescript-eslint/parser": "2.15.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "24.9.0",
    "node-mocks-http": "1.8.0",
    "prettier": "1.18.2",
    "ts-jest": "24.3.0",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.7.4"
  }
}
